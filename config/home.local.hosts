$ORIGIN home.local.
$TTL	86400 
@	SOA	ns1.home.local.	hostmaster.home.local. (
		20200709   ; serial
		21600      ; refresh after 6 hours
		3600       ; retry after 1 hour
		604800     ; expire after 1 week
		86400 )    ; minimum TTL of 1 day
;
;
; DNS BIND9 server(s)
           NS    ns1.home.local. 
ns1        A     192.168.1.3
;
; Kerberose / LDAP server
auth                   A               192.168.1.4;
;
; Network clients with DHCP reservations
router                 A               192.168.1.1 ;
srv1                   A               192.168.1.2 ;
rpi2                   A               192.168.1.3 ;
rpi3                   A               192.168.1.4 ;
xbox                   A               192.168.1.5 ;
phone2GHZ              A               192.168.1.6 ;
phone5GHZ              A               192.168.1.7 ;
laptop                 A               192.168.1.8 ;
tablet                 A               192.168.1.9 ;
ipad                   A               192.168.1.10 ;
tv                     A               192.168.1.11 ;
;
; Server web app services
www                    CNAME   ns1.home.local.
mysql                  CNAME   ns1.home.local.
;